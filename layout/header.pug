                </svg>
script.
    const headerBlock = document.querySelector(".header-block")
    try {
        const burgerBtn = document.querySelector(".header-block_mob svg:last-of-type");

        const headerClose = document.querySelector(".logo_x")

        burgerBtn.addEventListener("click", (e) => {
            e.stopPropagation()
            headerBlock.classList.toggle("active")
        })
        headerClose.addEventListener("click", () => {
            headerBlock.classList.remove("active")
        })
        document.addEventListener("click", (e) => {
            if (e.target !== headerBlock) {
                if (headerBlock.classList.contains("active")) {
                    headerBlock.classList.remove("active")
                }
            }
        })
    } catch(err) {
        console.warn("Требуется мобильная версия")
    }
    window.addEventListener("resize", () => {
        if (window.innerWidth >= 1440) {
            if (headerBlock.classList.contains("active")) {
                headerBlock.classList.remove("active")
            }
        }
    })
